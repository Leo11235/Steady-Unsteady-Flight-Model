{
    "simulation settings" : {
        // must be set
        "simulation type": "hotfire",
        // overrides defaults
        "debug comments": "none", 
        "output units": "SI", // SI, IMP, or MRT (rocket team's own mishmash of SI and IMP)
        "show output graphs": true, // whether to show graphs once the simulation is done computing
        "same simulation data": true // whether to save simulation data
    },

    "simulation constants": { // used to override natural and simulation constants, eg: 
        "tolerated apogee difference": 1 // changes acceptable distance between rocket apogee and target apogee to 1 meter
