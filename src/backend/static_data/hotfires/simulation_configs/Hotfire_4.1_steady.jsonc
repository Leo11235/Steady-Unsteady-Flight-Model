    "rocket attributes" : {
        "oxidizer mass flow rate": 0.825, // kg/s
        "chamber pressure": 2633000, // Pa
        "fuel external radius": 0.085725, // m
        "initial internal fuel radius": 0.064, // m; only used for hotfire tests ///// 'real' value should be 0.064
        "fuel length": 0.6096, // m

        // can be anything
        "launch site altitude": 364, // m // try to approximate testsite altitude
        "dry mass": 1, // kg // put anything here

        // PROPEP stuff here
        "liquid oxidizer type": "NITROUS OXIDE",
        "oxidizer molecule code": 657,
        "solid fuel type": "EICOSANE (PARAFFIN)",
        "fuel molecule code": 1029,

        // from literature
        "fuel grain density": 900, // kg/m^3
        "regression rate scaling coefficient": 0.000132, // [.]
        "regression rate exponent": 0.555 // [.]
        }
